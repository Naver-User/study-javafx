<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox prefHeight="400.0" prefWidth="340.0" spacing="5.0" xmlns="http://javafx.com/javafx/21.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.zerock.myapp.controller.RootController18">

   <padding>
      <Insets topRightBottomLeft="10.0" />
   </padding>

   <children>
      <Label text="Text Area 1">
         <font>
            <Font name="Lucida Sans Unicode" size="14.0" />
         </font>
      </Label>
      <TextArea fx:id="textArea1" prefHeight="200.0" prefWidth="200.0" wrapText="true">
         <font>
            <Font name="Lucida Sans Unicode" size="14.0" />
         </font>
      </TextArea>

      <Label text="Text Area 2">
         <font>
            <Font name="Lucida Sans Unicode" size="14.0" />
         </font>
      </Label>
      <TextArea fx:id="textArea2" prefHeight="200.0" prefWidth="200.0" wrapText="true">
         <font>
            <Font name="Lucida Sans Unicode" size="14.0" />
         </font></TextArea>
   </children>
</VBox>
