<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>


<HBox
    alignment="CENTER"
    spacing="10.0"
    prefHeight="26.0"
    prefWidth="486.0"

    fx:id="containerHBox"
    fx:controller="org.zerock.myapp.controller.RootController27"

    xmlns="http://javafx.com/javafx/21.0.1"
    xmlns:fx="http://javafx.com/fxml/1">

    <children>
        <Button onAction="#handleOpenFileChooser" text="Open FileChooser" fx:id="btnOpenFileChooser">
            <font><Font name="Lucida Sans Unicode" size="10.0" /></font>
        </Button>

        <Button onAction="#handleSaveFileChooser" text="Save FileChooser">
            <font><Font name="Lucida Sans Unicode" size="10.0" /></font>
        </Button>

        <Button onAction="#handleDirectoryChooser" text="DirectoryChooser">
            <font><Font name="Lucida Sans Unicode" size="10.0" /></font>
        </Button>

        <Button onAction="#handlePopup" text="Popup">
            <font><Font name="Lucida Sans Unicode" size="10.0" /></font>
        </Button>

        <Button onAction="#handleCustomDialog" text="CustomDialog">
            <font><Font name="Lucida Sans Unicode" size="10.0" /></font>
        </Button>
    </children>

    <padding>
        <Insets topRightBottomLeft="10.0" />
    </padding>

</HBox>
