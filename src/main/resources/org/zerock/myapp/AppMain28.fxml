<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>


<HBox alignment="CENTER" prefHeight="50.0" prefWidth="400.0" spacing="20.0" xmlns="http://javafx.com/javafx/21.0.1" xmlns:fx="http://javafx.com/fxml/1">

   <children>

      <Label style="-fx-background-color: black; -fx-text-fill: white;" text="검정바탕,노란글씨">
         <font><Font name="Lucida Sans Unicode" size="12.0" /></font>
         <padding><Insets topRightBottomLeft="5.0" /></padding>
         <cursor><Cursor fx:constant="HAND" /></cursor>
      </Label>

      <Label style="-fx-background-color: blue; -fx-text-fill: white;" text="파란바탕,흰글씨">
         <font><Font name="Lucida Sans Unicode" size="12.0" /></font>
         <padding><Insets topRightBottomLeft="5.0" /></padding>
         <cursor><Cursor fx:constant="OPEN_HAND" /></cursor>
      </Label>

      <Label style="-fx-background-color: red; -fx-text-fill: white;" text="빨간바탕,흰글씨">
         <font><Font name="Lucida Sans Unicode" size="12.0" /></font>
         <padding><Insets topRightBottomLeft="5.0" /></padding>
         <cursor><Cursor fx:constant="CLOSED_HAND" /></cursor>
      </Label>

   </children>

</HBox>
