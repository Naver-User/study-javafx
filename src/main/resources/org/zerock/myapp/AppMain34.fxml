<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>


<HBox alignment="CENTER" fillHeight="false" prefHeight="100.0" prefWidth="300.0" spacing="40.0"
      xmlns="http://javafx.com/javafx/21.0.1">
   <padding>
      <Insets topRightBottomLeft="10.0" />
   </padding>

   <children>
      <Button id="btn1" prefHeight="50.0" prefWidth="100.0" text="DropShadow">
         <font><Font name="Lucida Sans Unicode" size="13.0" /></font>
      </Button>

      <Button id="btn2" prefHeight="50.0" prefWidth="100.0" text="InnerShadow">
         <font><Font name="Lucida Sans Unicode" size="13.0" /></font>
      </Button>
   </children>
</HBox>
